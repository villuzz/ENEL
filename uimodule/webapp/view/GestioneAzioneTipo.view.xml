                    <Input value="{sSelect>/DurataCiclo}" maxLength="4"/>
                  </form:fields>
                </form:FormElement>
                <form:FormElement label="{i18n>Frequenza}">
                  <form:fields>
                     <Select xmlns="sap.m" id="onlyActive" selectedKey="{sSelect>/Frequenza}">
                        <items>
                          <core:ListItem xmlns:core="sap.ui.core" key="" text="" />
                          <core:ListItem xmlns:core="sap.ui.core" key="C" text="C" />
                          <core:ListItem xmlns:core="sap.ui.core" key="T" text="T"/>
                        </items>
                      </Select>
                  </form:fields>
                </form:FormElement>
                <form:FormElement label="{i18n>FlagAttivo}">
                  <form:fields>
                     <Select xmlns="sap.m" id="onlyActive" selectedKey="{sSelect>/FlagAttivo}">
                        <items>
