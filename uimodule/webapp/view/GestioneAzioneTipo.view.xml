                        <Column width="15em" id="col52" app:property="AggrActComponent"><Text text="{i18n>AggrActComponent}"/></Column>
                        <Column width="15em" id="col53" app:property="ClassActType"><Text text="{i18n>ClassActType}"/></Column>
                    </columns>
