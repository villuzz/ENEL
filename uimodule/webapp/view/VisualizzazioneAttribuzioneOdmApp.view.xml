<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" xmlns:fb="sap.ui.comp.filterbar" xmlns:l="sap.ui.layout" xmlns:ui="sap.ui.table" xmlns:f="sap.f" xmlns:form="sap.ui.layout.form" controllerName="PM030.APP1.controller.VisualizzazioneAttribuzioneOdmApp">
<NavContainer
			id="navCon"
			class="navContainerControl sapUiSmallMarginBottom">
    <f:DynamicPage id="dynamicPageId" preserveHeaderStateOnScroll="true" showFooter="false" fitContent="true" class="sapUiContentPadding" >
        <f:title>
            <f:DynamicPageTitle>
                <f:content>
                <HBox height="100%" alignItems="Center" alignContent="Center" justifyContent="Center">
                        <Button type="Emphasized" text="Home"  press="onBack" class="sapUiSmallMarginEnd"></Button>
                        <Title text="{i18n>TitoloVAOA}" class="sapUiTinyMarginTop"/>
                    </HBox>
                </f:content>
                <f:actions></f:actions>
                <f:navigationActions>
                    <Button text="Nuovo"  icon="sap-icon://document" press="onNuovo" class="sapUiTinyMarginBegin"/>
                    <Button text="Modifica" icon="sap-icon://edit" press="onModify" class="sapUiTinyMarginBegin"/>
                    <Button text="Copia"  icon="sap-icon://copy" press="onCopy" class="sapUiTinyMarginBegin"/>
                    <Button text="Cancella" icon="sap-icon://delete" press="onCancel" class="sapUiTinyMarginBegin"/>
                </f:navigationActions>
            </f:DynamicPageTitle>
        </f:title>
        <f:header>
            <f:DynamicPageHeader pinnable="true">
                <f:content>
                    <fb:FilterBar useToolbar="true" search="onSearchResult" showRestoreButton="true" showClearButton="true">
                        <fb:filterGroupItems>

                            <fb:FilterGroupItem id="index" visible="true" groupName="__$INTERNAL$" name="ind" label="{i18n>Index}" partOfCurrentVariant="true" visibleInFilterBar="true">
                              <fb:control>
                              <MultiComboBox id="cbIndex" items="{ path: 'sHelp>/T_APP_WO/IndexOdm', sorter: [{ path: 'DIVISIONE', descending: false, group:false }] }" > 
                                  <core:Item key="{sHelp>IndexOdm}" text="{sHelp>IndexOdm}" />
                                </MultiComboBox>
                              </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="cont" visible="true" groupName="__$INTERNAL$" name="cont" label="{i18n>Cont}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                <MultiComboBox id="cbCont" items="{ path: 'sHelp>/T_APP_WO/Zcount', sorter: [{ path: 'DIVISIONE', descending: false, group:false }] }" > 
                                  <core:Item key="{sHelp>Zcount}" text="{sHelp>Zcount}" />
                                </MultiComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="appuntamenti" visible="true" groupName="__$INTERNAL$" name="app" label="{i18n>Appuntamenti}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                <MultiComboBox id="cbAppuntamenti" items="{ path: 'sHelp>/T_APP_WO/Appuntam', sorter: [{ path: 'DIVISIONE', descending: false, group:false }] }" > 
                                  <core:Item key="{sHelp>Appuntam}" text="{sHelp>Appuntam}" />
                                </MultiComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="odm" visible="true" groupName="__$INTERNAL$" name="odm" label="{i18n>Odm}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                <MultiComboBox id="cbOdm" items="{ path: 'sHelp>/T_APP_WO/Aufpl', sorter: [{ path: 'DIVISIONE', descending: false, group:false }] }" > 
                                  <core:Item key="{sHelp>Aufpl}" text="{sHelp>Aufpl}" />
                                </MultiComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="statoODM" visible="true" groupName="__$INTERNAL$" name="sodm" label="{i18n>StatoODM}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                <MultiComboBox id="cbStatoODM" items="{ path: 'sHelp>/T_APP_WO/StatoOdm', sorter: [{ path: 'DIVISIONE', descending: false, group:false }] }" > 
                                  <core:Item key="{sHelp>StatoOdm}" text="{sHelp>StatoOdm}" />
                                </MultiComboBox>
                                    <!-- <Input type="Text" value="{FilterModel>/StatoODM}" maxLength="4"/> -->
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="dettaglioConferma" visible="true" groupName="__$INTERNAL$" name="dc" label="{i18n>DettaglioConferma}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                <MultiComboBox id="cbDettaglioConferma" items="{ path: 'sHelp>/T_APP_WO/DettConf', sorter: [{ path: 'DIVISIONE', descending: false, group:false }] }" > 
                                  <core:Item key="{sHelp>DettConf}" text="{sHelp>DettConf}" />
                                </MultiComboBox>
                                    <!-- <Input type="Text" value="{FilterModel>/DettaglioConferma}" maxLength="40"/> -->
                                </fb:control>
                            </fb:FilterGroupItem>

                        </fb:filterGroupItems>
                    </fb:FilterBar>
                     <OverflowToolbar class="sapUiMediumMarginTop" style="Clear">
                      <ToolbarSpacer/>
                      <Button text="Elimina pianificazione appuntamento" icon="sap-icon://delete" press="onElimina" class="sapUiTinyMarginBegin"/>
                       <Button text="Scarica" icon="sap-icon://download" press="onDataExport" class="sapUiTinyMarginBegin"/>
                      <Button text="Carica"  icon="sap-icon://upload" press="handleUploadPiani" class="sapUiTinyMarginBegin"/>
                      <Button text="Colonne" icon="sap-icon://action-settings" press="onPersoButtonPressed" class="sapUiTinyMarginBegin"/>
                    </OverflowToolbar>
                </f:content>
            </f:DynamicPageHeader>
        </f:header>
        <f:content>
            <ScrollContainer width="100%" height="100%" vertical="true" horizontal="true" >
                <Table id="tbVisualizzazioneAttribuzioneOdmApp" items="{path: 'T_APP_WO>/'}"  mode="MultiSelect" sticky="HeaderToolbar,ColumnHeaders" growing="true" growingThreshold="100" selectionChange="onSelectRow">
                  <columns>
                        <Column width="15em" id="col69" app:property="Zcount"><Text text="{i18n>Cont}"/></Column>
                        <Column width="15em" id="col70" app:property="Index"><Text text="{i18n>Index}"/></Column>
                        <Column width="15em" id="col71" app:property="Appuntamenti"><Text text="{i18n>Appuntamenti}"/></Column>
                        <Column width="15em" id="col72" app:property="Odm"><Text text="{i18n>Odm}"/></Column>
                        <Column width="15em" id="col73" app:property="Odm"><Text text="{i18n>CicloOperaz}"/></Column>
                        <Column width="15em" id="col74" app:property="NOper"><Text text="{i18n>NOper}"/></Column>
                        <Column width="15em" id="col75" app:property="NOper1"><Text text="{i18n>NOper1}"/></Column>
                        <Column width="15em" id="col76" app:property="NOper2"><Text text="{i18n>NOper2}"/></Column>
                        <Column width="15em" id="col77" app:property="NOper3"><Text text="{i18n>NOper3}"/></Column>
                        <Column width="15em" id="col78" app:property="NOper4"><Text text="{i18n>NOper4}"/></Column>
                        <Column width="15em" id="col79" app:property="NOper5"><Text text="{i18n>NOper5}"/></Column>
                        <Column width="15em" id="col80" app:property="Adm"><Text text="{i18n>Adm}"/></Column>
                        <Column width="15em" id="col81" app:property="StatoODM"><Text text="{i18n>StatoODM}"/></Column>
                        <Column width="15em" id="col82" app:property="DettaglioConferma"><Text text="{i18n>DettaglioConferma}"/></Column>
                        <Column width="15em" id="col83" app:property="DataPianificazione"><Text text="{i18n>DataPianificazione}"/></Column>
                        <Column width="15em" id="col84" app:property="FineCardine"><Text text="{i18n>FineCardine}"/></Column>
                        <Column width="15em" id="col85" app:property="ScadNaturale"><Text text="{i18n>ScadNaturale}"/></Column>
                        <Column width="15em" id="col86" app:property="Aggregatore"><Text text="{i18n>Aggregatore}"/></Column>
                        <Column width="15em" id="col87" app:property="DescrizioneAggregatore"><Text text="{i18n>DescrizioneAggregatore}"/></Column>
                    </columns>
                    <items>
                        <ColumnListItem vAlign="Middle" type="Active" press="onPressLine">
                            <cells>
                                <Text text="{T_APP_WO>Zcount}"/>
                                <Text text="{T_APP_WO>IndexOdm}"/>
                                <Text text="{T_APP_WO>Appuntam}"/>
                                <Text text="{T_APP_WO>Aufnr}"/>
                                <Text text="{T_APP_WO>Aufpl}"/>
                                <Text text="{T_APP_WO>Aplzl}"/>
                                <Text text="{T_APP_WO>Aplzl1}"/>
                                <Text text="{T_APP_WO>Aplzl2}"/>
                                <Text text="{T_APP_WO>Aplzl3}"/>
                                <Text text="{T_APP_WO>Aplzl4}"/>
                                <Text text="{T_APP_WO>Aplzl5}"/>
                                <Text text="{T_APP_WO>Qmnum}"/>
                                <Text text="{T_APP_WO>StatoOdm}"/>
                                <Text text="{T_APP_WO>DettConf}"/>
                                <Text text="{path: 'T_APP_WO>DataPian',
                                            type: 'sap.ui.model.type.Date',
                                            formatOptions: {
                                            pattern: 'dd/MM/yyyy'
                                            }}"/>
                                <Text text="{path: 'T_APP_WO>DataFineCard',
                                            type: 'sap.ui.model.type.Date',
                                            formatOptions: {
                                            pattern: 'dd/MM/yyyy'
                                            }}"/>
                                <Text text="{path: 'T_APP_WO>DataPianNatur',
                                            type: 'sap.ui.model.type.Date',
                                            formatOptions: {
                                            pattern: 'dd/MM/yyyy'
                                            }}"/>
                                <Text text="{T_APP_WO>Aggregatore}"/>
                                <Text text="{T_APP_WO>DescAggregatore}"/>                                  
                            </cells>
                        </ColumnListItem>
                    </items>
                </Table>
             </ScrollContainer>
        </f:content>
        <f:footer>
        
            <OverflowToolbar>
                <ToolbarSpacer/>
                <ToolbarSpacer/>
            </OverflowToolbar>
        </f:footer>
         <f:dependents>
      <core:Fragment fragmentName="PM030.APP1.view.fragment.UploadTable" type="XML"></core:Fragment>
    </f:dependents>
    </f:DynamicPage>
   

    <f:DynamicPage id="Detail" preserveHeaderStateOnScroll="true" showFooter="false" fitContent="true" class="sapUiContentPadding" >
        <f:title>
            <f:DynamicPageTitle>
                <f:content>
                 <HBox height="100%" alignItems="Center" alignContent="Center" justifyContent="Center">
                        <Button type="Transparent" icon="sap-icon://nav-back"  press="onBackDetail" class="sapUiSmallMarginEnd"></Button>
                        <Title title="{i18n>editPage}"  id="TitleDetail"  class="sapUiTinyMarginTop"/>
                    </HBox>
                </f:content>
                <f:actions></f:actions>
                <f:navigationActions>
                    <Button icon="sap-icon://save" press="onSave" text="{i18n>Salva}"/>
                </f:navigationActions>
            </f:DynamicPageTitle>
        </f:title>
        <f:header>
            <f:DynamicPageHeader pinnable="true">
                <f:content>
                 
                </f:content>
            </f:DynamicPageHeader>
        </f:header>
        <f:content>
     <form:Form id="FormToolbar"
          editable="true"
          ariaLabelledBy="Title1">
          <form:toolbar>
            <Toolbar id="TB1">
              <Title text="{i18n>editPage2}"/>
            </Toolbar>
          </form:toolbar>
          <form:layout>
          <form:ResponsiveGridLayout
            labelSpanXL="3"
            labelSpanL="3"
            labelSpanM="3"
            labelSpanS="12"
            adjustLabelSpan="false"
            emptySpanXL="4"
            emptySpanL="4"
            emptySpanM="4"
            emptySpanS="0"
            columnsXL="1"
            columnsL="1"
            columnsM="1"
            singleContainerFullSize="false" />
          </form:layout>
          <form:formContainers>
            <form:FormContainer ariaLabelledBy="Title2">
              <form:formElements>
                <form:FormElement label="{i18n>Cont}">
                  <form:fields>
                  <ComboBox  value="{sDetail>/Zcount}" enabled="{/Enabled}" items="{ path: 'sHelp>/T_APP_WO/Zcount', sorter: [{ path: 'DIVISIONENew', descending: false, group:false }] }" >
                   <core:Item key="{sHelp>Zcount}" text="{sHelp>Zcount}"/>
                   </ComboBox>
                  </form:fields>
                </form:FormElement>
                <form:FormElement label="{i18n>Index}" >
                  <form:fields>
                  <ComboBox  value="{sDetail>/IndexOdm}" enabled="{/Enabled}" items="{ path: 'sHelp>/T_APP_WO/IndexOdm', sorter: [{ path: 'DIVISIONENew', descending: false, group:false }] }" >
                   <core:Item key="{sHelp>IndexOdm}" text="{sHelp>IndexOdm}"/>
                   </ComboBox>
                  </form:fields>
                </form:FormElement>
                <form:FormElement label="{i18n>Appuntamenti}">
                  <form:fields>
                  <ComboBox  value="{sDetail>/Appuntam}" enabled="{/Enabled}" items="{ path: 'sHelp>/T_APP_WO/Appuntam', sorter: [{ path: 'DIVISIONENew', descending: false, group:false }] }" >
                   <core:Item key="{sHelp>Appuntam}" text="{sHelp>Appuntam}"/>
                   </ComboBox>
                  </form:fields>
                </form:FormElement>
                <form:FormElement label="{i18n>Odm}">
                  <form:fields>
                  <ComboBox  value="{sDetail>/Aufnr}" enabled="{/Enabled}" items="{ path: '', sorter: [{ path: 'DIVISIONENew', descending: false, group:false }] }" >
                   <core:Item key="{}" text="{}"/>
                   </ComboBox>
                  </form:fields>
                </form:FormElement>
                <form:FormElement label="{i18n>CicloOperaz}">
                  <form:fields>
                  <ComboBox  value="{sDetail>/Aufpl}" enabled="{/Enabled}" items="{ path: '', sorter: [{ path: 'DIVISIONENew', descending: false, group:false }] }" >
                   <core:Item key="{}" text="{}"/>
                   </ComboBox>
                  </form:fields>
                </form:FormElement>
                <form:FormElement label="{i18n>NOper}">
                <ComboBox  value="{sDetail>/Aplzl}" enabled="{/Enabled}" items="{ path: 'sHelp>/T_APP_WO/Aplzl', sorter: [{ path: 'DIVISIONENew', descending: false, group:false }] }" >
                   <core:Item key="{sHelp>Aufpl}" text="{sHelp>Aufpl}"/>
                   </ComboBox>
                </form:FormElement>
                <form:FormElement label="{i18n>NOper1}">
                <ComboBox  value="{sDetail>/Aplzl1}" enabled="{/Enabled}" items="{ path: 'sHelp>/T_APP_WO/Aplzl1', sorter: [{ path: 'DIVISIONENew', descending: false, group:false }] }" >
                   <core:Item key="{sHelp>Aplzl1}" text="{sHelp>Aplzl1}"/>
                   </ComboBox>
                </form:FormElement>
                <form:FormElement label="{i18n>NOper2}">
                <ComboBox  value="{sDetail>/Aplzl2}" enabled="{/Enabled}" items="{ path: 'sHelp>/T_APP_WO/Aplzl2', sorter: [{ path: 'DIVISIONENew', descending: false, group:false }] }" >
                   <core:Item key="{sHelp>Aplzl2}" text="{sHelp>Aplzl2}"/>
                   </ComboBox>
                </form:FormElement>
                <form:FormElement label="{i18n>NOper3}">
                <ComboBox  value="{sDetail>/Aplzl3}" enabled="{/Enabled}" items="{ path: 'sHelp>/T_APP_WO/Aplzl3', sorter: [{ path: 'DIVISIONENew', descending: false, group:false }] }" >
                   <core:Item key="{sHelp>Aplzl3}" text="{sHelp>Aplzl3}"/>
                   </ComboBox>
                </form:FormElement>
                <form:FormElement label="{i18n>NOper4}">
                <ComboBox  value="{sDetail>/Aplzl4}" enabled="{/Enabled}" items="{ path: 'sHelp>/T_APP_WO/Aplzl4', sorter: [{ path: 'DIVISIONENew', descending: false, group:false }] }" >
                   <core:Item key="{sHelp>Aplzl4}" text="{sHelp>Aplzl4}"/>
                   </ComboBox>
                </form:FormElement>
                <form:FormElement label="{i18n>NOper5}">
                <ComboBox  value="{sDetail>/Aplzl5}" enabled="{/Enabled}" items="{ path: 'sHelp>/T_APP_WO/Aplzl5', sorter: [{ path: 'DIVISIONENew', descending: false, group:false }] }" >
                   <core:Item key="{sHelp>Aplzl5}" text="{sHelp>Aplzl5}"/>
                   </ComboBox>
                </form:FormElement>
                <form:FormElement label="{i18n>Adm}">
                <ComboBox  value="{sDetail>/Qmnum}" enabled="{/Enabled}" items="{ path: 'sHelp>/T001W/DivisioneNew', sorter: [{ path: 'DIVISIONENew', descending: false, group:false }] }" >
                   <core:Item key="{sHelp>Fieldname1}" text="{sHelp>Fieldname1}"/>
                   </ComboBox>
                </form:FormElement>
                <form:FormElement label="{i18n>StatoODM}">
                <ComboBox  value="{sDetail>/StatoOdm}" enabled="{/Enabled}" items="{ path: 'sHelp>/T_APP_WO/StatoOdm', sorter: [{ path: 'DIVISIONENew', descending: false, group:false }] }" >
                   <core:Item key="{sHelp>StatoOdm}" text="{sHelp>StatoOdm}"/>
                   </ComboBox>
                </form:FormElement>
                <form:FormElement label="{i18n>DettaglioConferma}">
                <ComboBox  value="{sDetail>/DettConf}" enabled="{/Enabled}" items="{ path: 'sHelp>/T_APP_WO/DettConf', sorter: [{ path: 'DIVISIONENew', descending: false, group:false }] }" >
                   <core:Item key="{sHelp>DettConf}" text="{sHelp>DettConf}"/>
                   </ComboBox>                
                </form:FormElement>
                <form:FormElement label="{i18n>DataPianificazione}">
                  <DatePicker placeholder="Inserisci giorno" id="dataPianificazione" class="sapUiSmallMarginBottom" displayFormat="dd-MM-YYYY" dateValue="{sDetail>/DataPian}"/>
                </form:FormElement>
                <form:FormElement label="{i18n>FineCardine}">
                  <DatePicker placeholder="Inserisci giorno" id="dataFineCard" class="sapUiSmallMarginBottom" displayFormat="dd-MM-YYYY" dateValue="{sDetail>/DataFineCard}"/>
                </form:FormElement>
                <form:FormElement label="{i18n>ScadNaturale}">
                  <DatePicker placeholder="Inserisci giorno" id="scadNaturale" class="sapUiSmallMarginBottom" displayFormat="dd-MM-YYYY" dateValue="{sDetail>/DataPianNatur}"/>
                </form:FormElement>
                <form:FormElement label="{i18n>Aggregatore}">
                <ComboBox value="{sDetail>/Aggregatore}" enabled="{/Enabled}" items="{ path: 'sHelp>/T001W/DivisioneNew', sorter: [{ path: 'DIVISIONENew', descending: false, group:false }] }" >
                   <core:Item key="{sHelp>Fieldname1}" text="{sHelp>Fieldname1}"/>
                   </ComboBox>
                </form:FormElement>
                <form:FormElement label="{i18n>DescrizioneAggregatore}">
                <ComboBox  value="{sDetail>/DescAggregatore}" enabled="{/Enabled}" items="{ path: 'sHelp>/T001W/DivisioneNew', sorter: [{ path: 'DIVISIONENew', descending: false, group:false }] }" >
                   <core:Item key="{sHelp>Fieldname1}" text="{sHelp>Fieldname1}"/>
                   </ComboBox>
                </form:FormElement>
              </form:formElements>
            </form:FormContainer>
          </form:formContainers>
        </form:Form>
        
        </f:content>
    </f:DynamicPage>
  </NavContainer>
</mvc:View>