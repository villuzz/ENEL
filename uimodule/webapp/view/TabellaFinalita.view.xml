<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" xmlns:fb="sap.ui.comp.filterbar" xmlns:l="sap.ui.layout" xmlns:ui="sap.ui.table" xmlns:f="sap.f" xmlns:form="sap.ui.layout.form" controllerName="PM030.APP1.controller.TabellaFinalita">
<NavContainer
			id="navCon"
			class="navContainerControl sapUiSmallMarginBottom">
    <f:DynamicPage id="dynamicPageId" preserveHeaderStateOnScroll="true" showFooter="false" fitContent="true" class="sapUiContentPadding" >
        <f:title>
            <f:DynamicPageTitle>
                <f:content>
                     <HBox height="100%" alignItems="Center" alignContent="Center" justifyContent="Center">
                         <Button type="Emphasized" text="Home"  press="onBack" class="sapUiSmallMarginEnd"/>
                        <Title text="{i18n>TitoloF}" class="sapUiTinyMarginTop"/>
                    </HBox>
                </f:content>
                <f:actions></f:actions>
              <f:navigationActions>
                    <Button text="Nuovo"  icon="sap-icon://document" press="onNuovo" class="sapUiTinyMarginBegin"/>
                    <Button text="Modifica" icon="sap-icon://edit" press="onModify" class="sapUiTinyMarginBegin"/>
                    <Button text="Copia"  icon="sap-icon://copy" press="onCopy" class="sapUiTinyMarginBegin"/>
                    <Button text="Cancella" icon="sap-icon://delete" press="onCancel" class="sapUiTinyMarginBegin"/>   
                </f:navigationActions>
            </f:DynamicPageTitle>
        </f:title>
        <f:header>
            <f:DynamicPageHeader pinnable="true">
                <f:content>
                    <fb:FilterBar id="filterbarPiste" useToolbar="false" search="onSearchResult" showRestoreButton="true" showClearButton="true">
                        <fb:filterGroupItems>

                            <fb:FilterGroupItem id="divisione" visible="true" groupName="__$INTERNAL$" name="div" label="{i18n>Divisione}" partOfCurrentVariant="true" visibleInFilterBar="true">
                              <fb:control>
                               <MultiComboBox id="cbDivisione"  items="{ path: 'sHelp>/T_TP_MAN1/Divisione', sorter: [{ path: 'T_TP_MAN1', descending: false, group:false }] }" > 
                                    <core:Item key="{sHelp>Divisione}" text="{sHelp>Divisione}"/>
                                </MultiComboBox> 
                              </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="finalita" visible="true" groupName="__$INTERNAL$" name="fin" label="{i18n>Finalita}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                 <MultiComboBox id="cbFinalita"  items="{ path: 'sHelp>/T_TP_MAN1/TipoGestione1', sorter: [{ path: 'T_TP_MAN1', descending: false, group:false }] }" > 
                                    <core:Item key="{sHelp>TipoGestione1}" text="{sHelp>TipoGestione1}"/>
                                </MultiComboBox> 
                                    
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="raggruppamento" visible="true" groupName="__$INTERNAL$" name="ragg" label="{i18n>Raggruppamento}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                 <MultiComboBox id="cbRaggruppamento"  items="{ path: 'sHelp>/T_TP_MAN1/Raggruppamento', sorter: [{ path: 'T_TP_MAN1', descending: false, group:false }] }" > 
                                    <core:Item key="{sHelp>Raggruppamento}" text="{sHelp>Raggruppamento}"/>
                                </MultiComboBox> 
                                </fb:control>
                            </fb:FilterGroupItem>

                        </fb:filterGroupItems>
                    </fb:FilterBar>
                    <OverflowToolbar class="sapUiSmallMarginTop" style="Clear">
                      <ToolbarSpacer/>
                      <Button text="Scarica" icon="sap-icon://download" press="onDataExport" class="sapUiTinyMarginBegin"/>
                      <Button text="Carica"  icon="sap-icon://upload" press="handleUploadPiani" class="sapUiTinyMarginBegin"/>
                      <Button text="Colonne" icon="sap-icon://action-settings" press="onPersoButtonPressed" class="sapUiTinyMarginBegin"/>
                    </OverflowToolbar>
                </f:content>
            </f:DynamicPageHeader>
        </f:header>
        <f:content>
            <ScrollContainer width="100%" height="100%" vertical="true" horizontal="true" >
                <Table id="tbTabellaFinalita" items="{path: 'T_TP_MAN1>/'}"  mode="MultiSelect" sticky="HeaderToolbar,ColumnHeaders" selectionChange="onSelectRow">
                  <columns>
                        <Column width="15em" id="col61" app:property="TipoGestione1"><Text text="{i18n>Finalita}"/></Column>
                        <Column width="15em" id="col62" app:property="Divisione"><Text text="{i18n>Divisione}"/></Column>
                        <Column width="15em" id="col63" app:property="DesTipoGest1"><Text text="{i18n>DescrizioneFinalita}"/></Column>
                        <Column width="15em" id="col64" app:property="Raggruppamento"><Text text="{i18n>Raggruppamento}"/></Column>
                    </columns>
                    <items>
                        <ColumnListItem vAlign="Middle" type="Active" press="onPressLine">
                            <cells>
                                <Text text="{T_TP_MAN1>TipoGestione1}"/>
                                <Text text="{T_TP_MAN1>Divisione}"/>
                                <Text text="{T_TP_MAN1>DesTipoGest1}"/>
                                <Text text="{T_TP_MAN1>Raggruppamento}"/>
                            </cells>
                        </ColumnListItem>
                    </items>
                </Table>
             </ScrollContainer>
        </f:content>
        <f:footer>
            <OverflowToolbar>
                <ToolbarSpacer/>
                <ToolbarSpacer/>
            </OverflowToolbar>
        </f:footer>
          <f:dependents>
      <core:Fragment fragmentName="PM030.APP1.view.fragment.UploadTable" type="XML"></core:Fragment>
    </f:dependents>
    </f:DynamicPage>


    <f:DynamicPage id="Detail" preserveHeaderStateOnScroll="true" showFooter="false" fitContent="true" class="sapUiContentPadding" >
        <f:title>
            <f:DynamicPageTitle>
                <f:content>
                 <HBox height="100%" alignItems="Center" alignContent="Center" justifyContent="Center">
                        <Button type="Transparent" icon="sap-icon://nav-back"  press="onBackDetail" class="sapUiSmallMarginEnd"></Button>
                        <Title title="{i18n>editPage}"  id="TitleDetail"  class="sapUiTinyMarginTop"/>
                    </HBox>
                </f:content>
                <f:actions></f:actions>
                <f:navigationActions>
                    <Button icon="sap-icon://save" press="onSave" text="{i18n>Salva}"/>
                </f:navigationActions>
            </f:DynamicPageTitle>
        </f:title>
        <f:header>
            <f:DynamicPageHeader pinnable="true">
                <f:content>
                 
                </f:content>
            </f:DynamicPageHeader>
        </f:header>
        <f:content>
      
           <form:Form id="FormToolbar"
          editable="true"
          ariaLabelledBy="Title1">
          <form:toolbar>
            <Toolbar id="TB1">
              <Title text="{i18n>editPage2}"/>
            </Toolbar>
          </form:toolbar>
          <form:layout>
          <form:ResponsiveGridLayout
            labelSpanXL="3"
            labelSpanL="3"
            labelSpanM="3"
            labelSpanS="12"
            adjustLabelSpan="false"
            emptySpanXL="4"
            emptySpanL="4"
            emptySpanM="4"
            emptySpanS="0"
            columnsXL="1"
            columnsL="1"
            columnsM="1"
            singleContainerFullSize="false" />
          </form:layout>
          <form:formContainers>
            <form:FormContainer ariaLabelledBy="Title2">
              <form:formElements>
                <form:FormElement label="{i18n>Finalita}">
                  <form:fields>
                     <ComboBox  value="{sDetail>/TipoGestione1}" enabled="{oDataModel>/Enabled}" items="{ path: 'sHelp>/T_TP_MAN1/TipoGestione1', sorter: [{ path: 'T_TP_MAN1', descending: false, group:false }] }" >
                   <core:Item key="{sHelp>TipoGestione1}" text="{sHelp>TipoGestione1}"/>
                  </ComboBox>
                  </form:fields>
                </form:FormElement>
                <form:FormElement label="{i18n>Divisione}" >
                  <form:fields>
                     <ComboBox  value="{sDetail>/Divisione}" enabled="{oDataModel>/Enabled}" items="{ path: 'sHelp>/T001W/DivisioneNew', sorter: [{ path: 'DIVISIONENew', descending: false, group:false }] }" >
                   <core:Item key="{sHelp>Fieldname1}" text="{sHelp>Fieldname1}"/>
                  </ComboBox>
                  </form:fields>
                </form:FormElement>
                <form:FormElement label="{i18n>DescrizioneFinalita}">
                  <form:fields>
                      <ComboBox  value="{sDetail>/DesTipoGest1}" items="{ path: '', sorter: [{ path: '', descending: false, group:false }] }" >
                   <core:Item key="{}" text="{}"/>
                  </ComboBox>
                  </form:fields>
                </form:FormElement>
                <form:FormElement label="{i18n>Raggruppamento}">
                  <form:fields>
                     <ComboBox  value="{sDetail>/Raggruppamento}"  items="{ path: 'sHelp>/T_RAGGR/Raggruppamento', sorter: [{ path: 'aRAGGRUPPAMENTO', descending: false, group:false }] }" >
                   <core:Item key="{sHelp>Raggruppamento}" text="{sHelp>Raggruppamento}"/>
                  </ComboBox>
                  </form:fields>
                </form:FormElement>
              </form:formElements>
            </form:FormContainer>
          </form:formContainers>
        </form:Form>
        </f:content>
    </f:DynamicPage>
  </NavContainer>
</mvc:View>